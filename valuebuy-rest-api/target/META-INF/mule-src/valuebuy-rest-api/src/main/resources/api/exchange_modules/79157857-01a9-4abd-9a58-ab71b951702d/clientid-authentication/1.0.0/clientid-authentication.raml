#%RAML 1.0 SecurityScheme
type: x-client-id
describedBy:
  headers:
    client_id:
      type: string
      required: true
      example: ugtfrdedrdrdresrsyfugnj
    client_secret:
      type: string
      required: true
      example: vgcfokijnhuygtfrdsemk
  responses:
      401:
        body:
          application/json:
            example:
              error: Either password or username is wrong.